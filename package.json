{
  "name": "plain",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "node server/index.js --trace-warnings && vue-cli-service serve",
    "heroku-postbuild": "cross-env NODE_ENV=production vue-cli-service build",
    "start:server": "NODE_ENV=development nodemon server/index.js --trace-warnings",
    "start:client": "NODE_ENV=development npm install && vue-cli-service serve",
    "build": "cross-env NODE_ENV=production npm install",
    "test:unit": "vue-cli-service test:unit",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "axios": "^1.4.0",
    "bcrypt": "^5.1.0",
    "buefy": "^0.9.23",
    "compression": "^1.7.4",
    "connect-mongo": "^3.2.0",
    "core-js": "^3.6.5",
    "cors": "^2.8.5",
    "dotenv-webpack": "^8.0.1",
    "express": "^4.17.1",
    "express-flash": "0.0.2",
    "express-session": "^1.17.1",
    "howler": "^2.2.3",
    "http-status-codes": "^2.2.0",
    "joi": "^14.3.1",
    "mongoose": "^5.9.14",
    "morgan": "^1.10.0",
    "slugs": "^0.1.3",
    "uuid": "^9.0.0",
    "vue": "^2.6.11",
    "vue-router": "^3.2.0",
    "vue-toast-notification": "^1.0.1",
    "vue-unicons": "^3.3.1",
    "vuedraggable": "^2.24.3",
    "vuex": "^3.4.0",
    "vuex-persist": "^3.1.3"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.15",
    "@vue/cli-plugin-eslint": "~4.5.15",
    "@vue/cli-plugin-router": "~4.5.15",
    "@vue/cli-plugin-unit-jest": "~4.5.15",
    "@vue/cli-plugin-vuex": "~4.5.15",
    "@vue/cli-service": "^4.5.19",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/test-utils": "^1.0.3",
    "babel-eslint": "^10.1.0",
    "cross-env": "^7.0.3",
    "dotenv": "^4.0.0",
    "eslint": "^6.7.2",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-vue": "^6.2.2",
    "nodemon": "^2.0.22",
    "prettier": "^2.2.1",
    "sass": "^1.26.5",
    "sass-loader": "^8.0.2",
    "vue-template-compiler": "^2.6.11"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended",
      "@vue/prettier"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {},
    "overrides": [
      {
        "files": [
          "**/__tests__/*.{j,t}s?(x)",
          "**/tests/unit/**/*.spec.{j,t}s?(x)"
        ],
        "env": {
          "jest": true
        }
      }
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "jest": {
    "preset": "@vue/cli-plugin-unit-jest"
  }
}
